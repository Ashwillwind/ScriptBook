<Configuration>
<Info Description="Download and Install Office 2024 Pro Plus VL" />

<Add SourcePath="F:\ODT2024" OfficeClientEdition="32" Channel="PerpetualVL2024" MigrateArch="TRUE">
    <Product ID="ProPlus2024Volume" PIDKEY="XJ2XN-FW8RK-P4HMP-DKDBV-GCVGB">
      <Language ID="en-us" />
      <Language ID="ru-ru" />

      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
      <ExcludeApp ID="OneNote" />
      <ExcludeApp ID="Publisher" />

    </Product>

    <Product ID="VisioPro2024Volume" PIDKEY="B7TN8-FJ8V3-7QYCP-HQPMV-YY89G">
      <Language ID="en-us" />
      <Language ID="ru-ru" />
    </Product>

    <Product ID="ProjectPro2024Volume" PIDKEY="FQQ23-N4YCY-73HQ3-FM9WC-76HF4">
      <Language ID="en-us" />
      <Language ID="ru-ru" />
    </Product>

    <Product ID="ProofingTools">
      <Language ID="ru-ru" />
      <Language ID="en-us" />
    </Product>

</Add>

  <Property Name="SharedComputerLicensing" Value="0" />
  <Property Name="FORCEAPPSHUTDOWN" Value="FALSE" />
  <Property Name="DeviceBasedLicensing" Value="0" />
  <Property Name="SCLCacheOverride" Value="0" />
  <Property Name="PinIconsToTaskbar" Value="TRUE" />
  <Property Name="AUTOACTIVATE" Value="0" />
  <Updates Enabled="TRUE" />
  <RemoveMSI />


  <AppSettings>
    <Setup Name="Company" Value="HT" />
    <User Key="software\microsoft\office\16.0\excel\options" Name="defaultformat" Value="51" Type="REG_DWORD" App="excel16" Id="L_SaveExcelfilesas" />
    <User Key="software\microsoft\office\16.0\powerpoint\options" Name="defaultformat" Value="27" Type="REG_DWORD" App="ppt16" Id="L_SavePowerPointfilesas" />
    <User Key="software\microsoft\office\16.0\word\options" Name="defaultformat" Value="" Type="REG_SZ" App="word16" Id="L_SaveWordfilesas" />
  </AppSettings>

  <Display Level="Full" AcceptEULA="TRUE" />

<!--
  <Logging Level="Full" Path="F:\ODT2024\LogsProPlus2024VL" />
-->
  <Logging Level="Standard" Path="F:\ODT2024\LogsProPlus2024VL" />

</Configuration>
